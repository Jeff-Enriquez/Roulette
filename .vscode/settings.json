{
    "dotnet.preferCSharpExtension": true,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/*.meta": true,
        "**/.vscode": true,
        "**/*.asset": true,
        "Library": true,
        "Temp": true,
        "Logs": true,
        "Packages": true,
        "ProjectSettings": true,
        "Assembly-CSharp*": true,
        "Assembly-obj": true,
        "obj": true,
        "UserSettings": true,
        ".vsconfig": true,
        "**/.vs": true
    },
    "dotnet.defaultSolution": "roulette.sln",
    "omnisharp.enableEditorConfigSupport": true,
    "dotnet.server.useOmnisharp": false
}